<keymap version="1" name="Windows 副本" parent="$default">
  <action id="$EditorTranslateAction">
    <keyboard-shortcut first-keystroke="shift ctrl y" />
    <keyboard-shortcut first-keystroke="ctrl numpad5" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt 1" />
    <keyboard-shortcut first-keystroke="ctrl numpad1" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl numpad3" />
  </action>
  <action id="EditorPageDown">
    <keyboard-shortcut first-keystroke="page_down" />
    <keyboard-shortcut first-keystroke="ctrl numpad6" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="ctrl numpad4" />
  </action>
  <action id="EditorStartNewLine">
    <keyboard-shortcut first-keystroke="shift enter" />
    <keyboard-shortcut first-keystroke="ctrl windows" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="ctrl numpad9" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="ctrl numpad7" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl numpad2" />
  </action>
  <action id="编辑器EditorTranslateAction">
    <keyboard-shortcut first-keystroke="ctrl numpad5" />
  </action>
</keymap>